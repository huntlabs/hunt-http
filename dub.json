    "dependencies": {
        "hunt": {"path":"../hunt"},
		"hunt-net": {"path":"../hunt-net"},
        "hunt-trace" : {"path":"../hunt-trace"}
    },
    "configurations": [
        {
            "name": "default",
            "versions": [
                "NoTLS"
            ],
            "subConfigurations": {
                "hunt-net": "default"
            }
        },
        {
            "name": "boringssl",
            "versions": [
                "WithTLS"
            ],
            "subConfigurations": {
                "hunt-net": "boringssl"
            }
        },
        {
            "name": "openssl",
            "versions": [
                "WithTLS"
            ],
            "subConfigurations": {
                "hunt-net": "openssl"
            }
        },
        {
            "name": "opensslv11",
            "versions": [
                "WithTLS"
            ],
            "subConfigurations": {
                "hunt-net": "opensslv11"
            }
        }
    ]
}