{
	"name": "http-demo",
	"description": "A demo for WebSocket Client/Server with Hunt-http.",
    "copyright": "Copyright (C) 2018-2019, HuntLabs",
    "homepage": "https://www.huntlabs.net/",
	"license": "Apache-2.0",
	"dependencies": {
		"hunt-http" :{"path": "../../"}
	},
	"versions": [
		"HUNT_DEBUG", "HUNT_IO_DEBUG", "HUNT_HTTP_DEBUG", "HUNT_HTTP_DEBUG_MORE"
	],
	"subConfigurations": {
		"hunt-http" : "tls"
	},
	"configurations": [
		{
			"name": "client",
			"targetName": "http-client",
			"targetType": "executable",
			"excludedSourceFiles": [
				"source/server.d"
			]
		},
		{
			"name": "server",
			"targetName": "http-server",
			"targetType": "executable",
			"excludedSourceFiles": [
				"source/client.d"
			]
		}
	]
}